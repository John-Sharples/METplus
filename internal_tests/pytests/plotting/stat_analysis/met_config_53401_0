






exit_on_warning = FALSE;




nc_compression = 0;


output_precision = 5;



tmp_dir = "/tmp";







K_to_C(t) = t - 273.15;
C_to_K(t) = t + 273.15;
C_to_F(t) = 1.8 * t + 32.0;
F_to_C(t) = (5.0 / 9.0) * ( t - 32.0 );
K_to_F(t) = C_to_F(K_to_C(t));
F_to_K(t) = C_to_K(F_to_C(t));

MM_to_IN(x) = x / 25.4;
IN_to_MM(x) = x * 25.4;

PA_to_HPA(p) = p / 100.0;
HPA_to_PA(p) = p * 100.0;
PA_to_MB(p)  = PA_to_HPA(p);
MB_to_PA(p)  = HPA_to_PA(p);











